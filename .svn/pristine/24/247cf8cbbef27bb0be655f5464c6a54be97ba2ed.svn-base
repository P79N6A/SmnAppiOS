iBAAiB,GAAC,KAAI,CAAC,YAAY,CAAC,KAAK,CAAC,CAAC;oBAChD,KAAI,CAAC,aAAa,GAAG,KAAK,CAAC;gBAC7B,CAAC;YACD,CAAC,CAAC,CAAC;YACH,IAAI,CAAC,OAAO,CAAC,GAAG,CAAC,UAAU,CAAC,CAAC,IAAI,CAAC,UAAC,GAAG;gBACpC,OAAO,CAAC,GAAG,CAAC,GAAG,CAAC,CAAC;YACjB,CAAC,CAAC,CAAC;QACP,CAAC;QAAC,IAAI,CAAC,CAAC;YACN,IAAI,CAAC,OAAO,CAAC,MAAM,CAAC,UAAU,CAAC,CAAC,IAAI,CAAC;gBACnC,KAAI,CAAC,aAAa,GAAG,KAAK,CAAC;gBAC3B,KAAI,CAAC,iBAAiB,GAAC,IAAI,CAAC;YAC9B,CAAC,CAAC,CAAC;YACH,IAAI,CAAC,OAAO,CAAC,GAAG,CAAC,UAAU,CAAC,CAAC,IAAI,CAAC,UAAC,GAAG;gBACpC,OAAO,CAAC,GAAG,CAAC,GAAG,CAAC,CAAC;YACjB,CAAC,CAAC,CAAC;QACP,CAAC;IACH,CAAC;IACD,qCAAiB,GAAjB;QACE,IAAI,CAAC,OAAO,CAAC,IAAI,CAAC,mBAAmB,CAAC,CAAC;IACzC,CAAC;IAxDU,SAAS;QAJrB,wEAAS,CAAC;YACT,QAAQ,EAAE,YAAY;WACG;SAC1B,CAAC;mNAQkC,EAAa;YAC7B,MAAmE;OAR1E,SAAS,CAyDrB;IAAD,CAAC;AAAA;SAzDY,SAAS,e","file":"10.js","sourcesContent":["import { NgModule } from '@angular/core';\nimport { IonicPageModule } from 'ionic-angular';\nimport { LoginPage } from './login';\n\n@NgModule({\n  declarations: [\n    LoginPage,\n  ],\n  imports: [\n    IonicPageModule.forChild(LoginPage),\n  ],\n})\nexport class LoginPageModule {}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/login/login.module.ts","import { Component,OnInit } from '@angular/core';\nimport { IonicPage, NavController, NavParams} from 'ionic-angular';\nimport { UsersProvider } from '../../providers/users/users';\nimport { Storage } from '@ionic/storage';\nimport { FormControl, FormGroup, Validators } from '@angular/forms';\n/**\n * Generated class for the LoginPage page.\n *\n * See https://ionicframework.com/docs/components/#navigation for more info on\n * Ionic pages and navigation.\n */\n\n@IonicPage()\n@Component({\n  selector: 'page-login',\n  templateUrl: 'login.html',\n})\nexport class LoginPage  implements OnInit {\n  loginform: FormGroup;\n  authenticated = false;\n  errorMsg = false;\n  public responseData:any=null;\n  public responseFailsData = [];\n  userData = {\"email\": \"\",\"password\": \"\"};\n  constructor(private usersProvider:UsersProvider,\n    public navCtrl: NavController, public navParams: NavParams,private storage: Storage) {\n      this.storage.get('my_token').then((val) => {\n        if(val!=null){\n          this.authenticated = true;\n        }        \n      });\n  }\n  ngOnInit() {\n    let EMAILPATTERN = /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-z\\-0-9]+\\.)+[a-z]{2,}))$/;\n    this.loginform = new FormGroup({\n      password: new FormControl('', [Validators.required, Validators.minLength(6)]),\n      email: new FormControl('', [Validators.required, Validators.pattern(EMAILPATTERN)]),\n    });\n  }\n  ionViewDidLoad() {\n  }\n  setAuthState(authenticated){\n      if(authenticated){\n      this.usersProvider.userLogin(this.userData)\n      .subscribe((response)=> {this.responseData=response;\n      if(this.responseData['status']!=0){\n        this.navCtrl.push('UserProfilePage',{data:this.responseData['data']['profile']['id']      \n        });\n        this.storage.set('my_token', this.responseData['data']['profile']['id']).then(() => {\n        this.authenticated = true; \n        this.errorMsg=false;\n        });\n      }else{\n        this.errorMsg=true;\n        this.responseFailsData=this.responseData['msg'];\n        this.authenticated = false;\n      }\n      });\n      this.storage.get('my_token').then((val) => {\n        console.log(val);\n        });\n    } else {\n      this.storage.remove('my_token').then(() => {\n        this.authenticated = false;\n        this.responseFailsData=null;\n      });\n      this.storage.get('my_token').then((val) => {\n        console.log(val);\n        });\n    }    \n  }\n  goToResetPassPage(){\n    this.navCtrl.push('ResetPasswordPage'); \n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/login/login.ts"],"sourceRoot":""}