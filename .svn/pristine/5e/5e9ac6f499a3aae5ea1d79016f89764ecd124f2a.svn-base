<ion-header>

  <ion-navbar>
      <button ion-button menuToggle>
          <ion-icon name="menu"></ion-icon>
        </button>
    <ion-title>Contact</ion-title>
  </ion-navbar>

</ion-header>


<ion-content padding>
	<!--<ion-title class="main-head">Contact Us</ion-title>-->
	<ion-title class="homelogo contact-logo" text-center><img src="../assets/imgs/login-logo.png" alt=""></ion-title>
	<ion-list class="login-inputs">
	
		<ion-item>
				<ion-label stacked>Enter Name</ion-label>
			<ion-input type="text" placeholder="Name"></ion-input>
		</ion-item>
	
		<ion-item>
				<ion-label stacked>Enter Email Id</ion-label>
			<ion-input type="email" placeholder="Email"></ion-input>
		</ion-item>
		<ion-item>
				<ion-label stacked>Enter Your Message</ion-label>
			<ion-textarea placeholder="Message"></ion-textarea>
		</ion-item>
		<button block color="primary" ion-button="" class="disable-hover button button-ios btn-login">SUBMIT</button>
	</ion-list>
</ion-content>
