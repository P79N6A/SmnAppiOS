page-baby-detail {
.details-profile.grid{padding:0px 0px 10px;border-bottom:1px solid #242525;margin:0px;}
.details-profile{
.card{margin:0px;
width:100%;
background:none;color: #ffffff;
border-radius: 0px;}
.card-content{padding:0px;font-style: italic;
h2{display: inline-block;
color: #fff;font-style: normal;}
}
}
.highestbid-cont{
.card{margin:0px;
width:100%;background:none;
}
.highest-bid{background:url(../assets/imgs/highest-bid-bg.png) no-repeat;
width: 100%;
background-size: cover;
padding: 22px 0px 10px;
height: 65px;
text-align: center;
h2{text-transform:uppercase;
color:#fff;font-size: 22px;
span{color:#c60b18; font-weight:bold;}
}
}
.auction-time h2{color:#fff;
text-align:center;
text-transform:uppercase;}
.item{padding:0px;background:none;
.item-inner{padding:0px;
.label{margin:0px;text-align:center;
ion-col{color:#c60b18;font-size: 18px; font-weight:bold;}
.time-txt ion-col{color:#fff;font-size: 14px;
padding: 0px;}
} 
}
}
}
.auction-time{padding:15px 0px 0px; margin:0px 0px 15px;}
.auction-timer.card-content,.place-bid.grid,.place-bid ion-col{padding:0px;}
.place-bid{
margin:0px 0px 10px;
input{margin:0px;
background:#1b1b1b;
border:1px solid #5a5c5d;
border-right:0px;
width: 100%;
color:#fff;
border-radius: 4px 0px 0px 4px;
padding: 6px 10px;}
button.button{margin-left: 0px;
margin-right: 0px;
border-radius: 0px 4px 4px 0px;
padding: 0px 9px 0px 8px;
font-size: 1.4rem;
height: 36px;}
}
.details-tabbing .toolbar-background{background:none;}
.details-tabbing .toolbar-content{
    background: #1b1b1b;
}
.details-tabbing.toolbar{
padding:0px;}
.details-tabbing{
 .segment-button.segment-activated {
 border:none;
    border-bottom: 3px solid #c60b18;
	color:#c60b18;
	background:none;
	}
	.segment-button{color:#fff;font-weight:bold;
	border:none;
	border-radius:0px !important;}
}
.babyDetails .card{margin:0px;width:100%;
background: #000000;}
.babyDetails .card-content{padding:0px;
color:#fff;
.grid{padding:0px;}
ul{padding:0px; margin:0px;
li{list-style:none;
margin-bottom: 10px;
font-size: 1.2rem;}
}
}
.profile-photos{
ion-col{padding:0px;}
.private-img a{margin:0px;
position:relative;
display:block;
height:100%;
background:#a8a8a8;
h3{position: absolute;
text-align: center;
color: #000;
margin: 0px;
top:15px;
font-size: 16px;
span{font-size:12px;
display:block;}
}
}
}
.profile-photos img{
height: 100%;}
}
.details-tabbing .segment{background: #1b1b1b;}
.details-tabbing .segment-button {
    height: 4.2rem;
    font-size: 1.2rem;
    line-height: 4rem;
}
.videoPlayer {
    width: 100%;
    height: 100%;
}
.public-videos .grid{padding-left:0px;
padding-right:0px;
.col{padding:0px;}}
.modal-wrapper .button{
    height: auto;
    font-size: 30px;
    background: none;
    margin: 0;
    padding: 0;
	font-weight: normal;
}